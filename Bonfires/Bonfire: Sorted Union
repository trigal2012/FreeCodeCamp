function unite(source, check1, check2, check3){
  var newArr = source;
  for(var x = 1; x<arguments.length; x++){
      for(var i = 0; i<arguments[x].length;i++){
          if(source.includes(arguments[x][i])=== false){
              newArr.push(arguments[x][i]);
             }
          }
    }
  console.log(newArr);
  return newArr;
}


unite([1, 3, 2], [5, 2, 1, 4], [2, 1]);
